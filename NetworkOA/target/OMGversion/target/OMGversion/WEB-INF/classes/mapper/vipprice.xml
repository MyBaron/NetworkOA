<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.Dao.vippriceDao">
    <select id="findOne" resultType="cn.Entity.vippriceJavaBean">
        SELECT * from  vipprice where vip_grade=#{vip_grade}
    </select>

    <select id="findAll" resultType="cn.Entity.vippriceJavaBean">
        SELECT * FROM vipprice
    </select>
</mapper>